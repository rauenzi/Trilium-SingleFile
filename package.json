{
  "name": "trilium-singlefile",
  "version": "0.1.0",
  "description": "An addon for Trilium to easily import SingleFile archives.",
  "homepage": "https://github.com/rauenzi/Trilium-SingleFile",
  "repository": {
    "type": "git",
    "url": "https://github.com/rauenzi/Trilium-SingleFile.git"
  },
  "keywords": [
    "trilium",
    "notes",
    "singlefile"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "build": "esrun scripts/build.ts",
    "build-main": "esrun scripts/build.ts -- --module=main",
    "build-ui": "esrun scripts/build.ts -- --module=ui",
    "build-styles": "esrun scripts/build.ts -- --module=styles",
    "build-settings": "esrun scripts/build.ts -- --module=ui --module=styles",
    "build-dist": "esrun scripts/build.ts -- --minify",
    "pack": "tpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@digitak/esrun": "^3.2.24",
    "@electron/remote": "^2.0.11",
    "@types/jquery": "^3.5.19",
    "@types/trilium": "npm:trilium-types@^0.1.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "dotenv": "^16.3.1",
    "electron": "^26.2.1",
    "esbuild": "^0.19.3",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-solid": "^0.13.0",
    "trilium-etapi": "^0.1.1",
    "trilium-pack": "^0.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "solid-js": "^1.7.11"
  }
}
